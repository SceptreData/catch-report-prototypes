n  font-size: 1.125rem;\n  padding: 0.5rem 1rem;\n  margin-bottom: 0.5rem;\n  width: 100%;\n  border: 1px solid black;\n  color: black;\n}\n\nselect {\n  border:1px solid black;\n  font-size: 1.125rem;\n  padding: 0.5rem 1rem;\n  margin-bottom: 0.5rem;\n}\n\n.prototypes {\n  display: flex;\n  justify-content: space-between;\n  @media screen and (max-width: 600px) {\n    flex-direction: column;\n  }\n}\n#add-fish {\n  box-shadow: $shadow;\n  background: $green;\n  border: 0;\n  padding: .5rem 1.125rem;\n  cursor: pointer;\n  border-radius: 3px;\n  text-decoration: underline;\n  color: white;\n  &:hover {\n    background: $light-green;\n  }\n}\n\n#list-quantity {\n  width: 80%;\n  color: black;\n}\n\n#list-fish {\n  width: 80%;\n}\n\n#multi-form {\n  display: none;\n}\n\n#error{\n  color: $red;\n}\n\nh5 {\n  color: rgb(63, 63, 63);\n  margin-bottom: .5rem;\n}\n\n.viewport {\n  box-shadow: $shadow;\n  border: 1px solid rgba(128, 128, 128, 0.616);\n  min-height: 100px;\n  width: 100%;\n  background: white;\n  margin-bottom: 1rem;\n  overflow:hidden;\n\n  .fish-entry {\n    width: 100%;\n    padding: .2rem 1rem;\n    button {\n      padding: .25rem .5rem;\n      text-align: center;\n    }\n    &:hover {\n      background: $light-green;\n      button {\n        visibility: visible;\n        cursor: pointer;\n        background: $red;\n        margin-left: auto;\n      }\n    }\n  }\n\n  button {\n    visibility: hidden;\n    border: 0;\n  }\n}\n\n.viewport.empty {\n  padding: 0.5rem 1rem;\n  color: rgb(63, 63, 63);\n}\n\n.species-entry {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 0.5rem;\n  width: 100%;\n}\n.species-entry > div {\n  display: flex;\n  flex-direction: column;\n  width: 25%;\n}\n\n.species-select {\n  width: 45% !important;\n}\n\n.species-quantity {\n  max-width: 75px;\n}\n\n.fish-inputs {\n  display: flex;\n  flex-wrap: wrap;\n}\n\n.fish-input {\n  display: flex;\n  align-items: center;\n  width: 50%;\n  color: black;\n  label {\n    width: 75px;\n    margin-right: 0.5rem;\n  }\n  input {\n    max-width: 50px;\n  }\n}\n\ntextarea {\n  margin-bottom: 1rem;\n}\n\n.submit {\n  padding: 1rem 2rem;\n  background: $green;\n  border: 0;\n  border-radius: 3px;\n  color: white;\n}\n\np {\n  padding-bottom: 0;\n  display: flex;\n  align-items: center;\n  width: 125px;\n  margin: 0.125rem;\n  margin-bottom: .25rem;\n  \n  .vp-species {\n    width: 25%;\n  }\n\n  .vp-quantity {\n    width: 10%;\n    text-align: right;\n  }\n}\n\n.fish-input input {\n  padding: .5rem;\n}"},"lineCount":null}},"error":null,"hash":"ff804dfd821bf761f28753a5d917a875","cacheData":{"env":{}}}